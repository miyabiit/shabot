<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link MyAccount.human_attribute_name(:bank), :bank %></th>
			<th><%= casein_sort_link MyAccount.human_attribute_name(:bank_branch), :bank_branch %></th>
			<th><%= casein_sort_link MyAccount.human_attribute_name(:category), :category %></th>
			<th><%= casein_sort_link MyAccount.human_attribute_name(:ac_no), :ac_no %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% my_accounts.each do |my_account| %>
		<tr>
			<td><%= casein_table_cell_link my_account.bank, casein_my_account_path(my_account) %></td>
			<td><%= casein_table_cell_link my_account.bank_branch, casein_my_account_path(my_account) %></td>
			<td><%= casein_table_cell_link my_account.category, casein_my_account_path(my_account) %></td>
			<td><%= casein_table_cell_link my_account.ac_no, casein_my_account_path(my_account) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_my_account_path(my_account), :method => :delete, :data => { :confirm => "Are you sure you want to delete this my account?" }) %>
			</td>
		</tr>
	<% end %>
</table>
