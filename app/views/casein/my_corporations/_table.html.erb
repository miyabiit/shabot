<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
  <thead>
    <tr>
      <% %W(code name).each do |attr_name| -%>
        <th><%= casein_sort_link controller.model.human_attribute_name(attr_name), attr_name %></th>
      <% end -%>
      <th>&nbsp;</th>
    </tr>
  </thead>
  
  <% my_corporations.each do |my_corporation| %>
    <tr class="<%= 'danger' if my_corporation.deleted? %>">
      <td><%= casein_table_cell_link my_corporation.code, casein_my_corporation_path(my_corporation) %></td>
      <td><%= casein_table_cell_link my_corporation.name, casein_my_corporation_path(my_corporation) %></td>
      <td class="delete">

        <% if !my_corporation.deleted? %>
          <%= table_row_trash_icon_link_to casein_my_corporation_path(my_corporation), :method => :delete, :data => { :confirm => t("messages.destroy_confirmation", model_name: controller.model.model_name.human) } %>
        <% end %>
      </td>
    </tr>
  <% end %>
</table>
